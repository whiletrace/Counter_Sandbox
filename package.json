{
  "name": "react-test",
  "version": "1.0.0",
  "description": "A place for testing react goodness",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint --format node_modules/eslint-stylish-config/stylish.js file.js",
    "start": "webpack-dev-server --inline --hot",
    "compile": "webpack",
    "test": "mocha --require babel-core/register --require ./tests/setup.js --recursive tests/**/*.spec.js",
    "test:debug": "npm run test -- debug",
    "test:dev": "npm run test -- --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.1",
    "enzyme": "^2.1.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-friendly-formatter": "^2.0.4",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.2",
    "eslint-stylish": "^0.2.0",
    "eslint-stylish-config": "0.0.2",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "react-router": "^2.2.4",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "redux": "^3.6.0"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danieljwest/HackUniversityReactStarter.git"
  },
  "bugs": {
    "url": "https://github.com/danieljwest/HackUniversityReactStarter/issues"
  },
  "homepage": "https://github.com/danieljwest/HackUniversityReactStarter#readme"
}
